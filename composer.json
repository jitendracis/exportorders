{
    "name": "jitendracis/exportorders",
    "type": "magento-module",
    "description": "Export order in excel file",
    "authors": [
        {
            "name": "Jitendra Mandloi",
            "email": "jitendra.m@cisinlabs.com"
        }
    ],
    "require": {
        "magento-hackathon/magento-composer-installer":"*"
    },
    "extra": {
        "magento-root-dir": "./",
        "magento-deploystrategy": "link",
        "map": [
          ["./app/design/adminhtml/default/default/layout/exportproduct.xml", "./app/design/adminhtml/default/default/layout/exportproduct.xml"],
          ["./app/design/frontend/base/default/layout/exportproduct.xml", "./app/design/frontend/base/default/layout/exportproduct.xml"],
          ["./app/design/frontend/base/default/template/exportproduct", "./app/design/frontend/base/default/template/exportproduct"],
          ["./app/etc/modules/Cis_Exportproduct.xml", "./app/etc/modules/Cis_Exportproduct.xml"],
          ["./app/code/local/Cis/Exportproduct", "./app/code/local/Cis/Exportproduct"],
          ["./lib/cis", "./lib/cis"],
          ["./app/locale/en_US/template/email/exportorders.html", "./app/locale/en_US/template/email/exportorders.html"]
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
